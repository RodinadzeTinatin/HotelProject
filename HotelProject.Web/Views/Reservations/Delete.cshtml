@model Reservation

<form asp-controller="Reservations" asp-action="DeletePOST" method="post">
    <input hidden asp-for="Id" />
    <div class="mb-3">
        <input class="form-control" asp-for="CheckInDate" disabled />
    </div>
    <div class="mb-3">
        <input class="form-control" asp-for="CheckOutDate" disabled />
    </div>
    
    <div class="mb-3">
        <input type="submit" class="btn btn-danger" value="წაშლა" />
    </div>
</form>